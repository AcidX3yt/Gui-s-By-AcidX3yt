local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Stranger Things: Tragedies Made By Project Acid", "Serpent")

local Tab = Window:NewTab("Characters")
local HumansSection = Tab:NewSection("Humans")

HumansSection:NewButton("Erica Sinclair", "", function()
    local args = {
        [1] = "Erica"
    }

    game:GetService("ReplicatedStorage"):FindFirstChild("Pick Character"):FireServer(unpack(args))
end)

HumansSection:NewButton("Chrissy", "", function()
    local args = {
        [1] = "Chrissy"
    }

    game:GetService("ReplicatedStorage"):FindFirstChild("Pick Character"):FireServer(unpack(args))
end)

