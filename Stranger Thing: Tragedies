local Tab = Window:NewTab("Powers")
local PowersSection = Tab:NewSection("Powers")

PowersSection:NewKeybind("Invisibility", "", Enum.KeyCode.N, function()
local args = {
    [1] = "invisibility",
    [2] = workspace.StarcourtMall:FindFirstChild("1BottomLevel").Floor,
    [3] = Vector3.new(844.8388671875, 75.6248550415039, 877.2667236328125)
}

game:GetService("ReplicatedStorage").Psychics.Kali:FindFirstChild("Kali Powers"):FireServer(unpack(args))
end)

PowersSection:NewKeybind("Swarm", "", Enum.KeyCode.M, function()
local args = {
    [1] = "swarm",
    [2] = workspace.StarcourtMall:FindFirstChild("1BottomLevel").CourtYard.PlantBench.WallFlowerPot,
    [3] = Vector3.new(868.5960083007812, 77.9154281616211, 862.0021362304688)
}

game:GetService("ReplicatedStorage").Psychics.Kali:FindFirstChild("Kali Powers"):FireServer(unpack(args))
end)

