local UI = loadstring(game:HttpGet("https://raw.githubusercontent.com/MagikManz/Venyx-UI-Library/main/source.lua"))()
local ui = UI.new("(Project Acid) The Vampire Legacies Booster Script")-- Gui name

local Misc = ui:Tab("Misc")
--local Witch = ui:Tab("Witch")
local Teleports = ui:Tab("Teleports")--Tabs
local Credits = ui:Tab("Credits")
local Settings = ui:Tab("To Toggle gui is Right Shift")

local Teleports = Teleports:Section("Teleports")

Misc:Button("GetCoins",function()
    local args = {
    [1] = "smallCoinGift"
}

game:GetService("ReplicatedStorage").Events.guiSpecialEvent:FireServer(unpack(args))
end)

Misc:Toggle("DeleteChannel", false, function(toggle)
_G.DeleteChannel = toggle
    while _G.DeleteChannel == true do
	    local Player = game.Players.LocalPlayer
    	local Character = Player.Character

    	local Channel = Character:WaitForChild("Channeling")
    	Channel:Destroy()
    	wait()
    end
end)
Misc:Slider("Speed", 0, 900, 16, function(value)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)
Misc:Slider("Jump Power", 0, 900, 50, function(value)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end)
Misc:Button("Infinite Yeild",function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)
Misc:Button("RemoteSpy",function()
    loadstring(game:HttpGet("https://pastebin.com/raw/70mkSi6G"))()
end)
