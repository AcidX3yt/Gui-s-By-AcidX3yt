local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Heroes Online World Gui (Project Acid)", "Serpent")

local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")

local Tab = Window:NewTab("Keybinds")
local KeybindsSection = Tab:NewSection("Keybinds")

local Tab = Window:NewTab("Player")
local PlayersSection = Tab:NewSection("Player")

local Tab = Window:NewTab("Admin")
local AdminsSection = Tab:NewSection("Admins")

local Tab = Window:NewTab("Credits")
local CreditsSection = Tab:NewSection("Credits")

KeybindsSection:NewToggle("Heal Others", "G is Keybind", function(state)
    if state then
        print("Toggle On")
        local function getPlayer()
        local userInput = game:GetService("UserInputService")
        local cam = workspace.CurrentCamera
        local mousePos = userInput:GetMouseLocation()
        local mouseRay = cam:ViewportPointToRay(mousePos.X, mousePos.Y)
        players = {}
        for _, x in next, game:GetService('Players'):GetPlayers() do
            players[#players + 1] = x.Character
        end
        local raycastParams = RaycastParams.new()
        raycastParams.FilterType = Enum.RaycastFilterType.Whitelist
        raycastParams.FilterDescendantsInstances = players
        raycastParams.IgnoreWater = true
        local raycastResult = workspace:Raycast(mouseRay.Origin, mouseRay.Direction * 1000, raycastParams)
        if raycastResult then
            for _, x in next, game:GetService('Players'):GetPlayers() do
                if not raycastResult.Instance:IsDescendantOf(x.Character) then
                    continue 
                end
                return x
            end
        end
        return false
    end
    
    local function Set(list)
        local set = {}
        for _, l in ipairs(list) do
            if l then
                set[l] = true
            end
        end
        return set
    end
    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
     if key == "g" then
    local player = getPlayer()
     if player then

local args = {
    [1] = "ExtraKey4",
    [2] = true,
    [3] = "CurrentAbility",
    [4] = player.Character
}

game:GetService("ReplicatedStorage").Knit.Services.AbilityService.RF.AbilityRequest:InvokeServer(unpack(args))
    end
    end
    end)
    else
        print("Toggle Off")
    end
end)

MainSection:NewButton("Redeem Codes", "redeams codes", function()
    print("Clicked")
    local args = {
    [1] = "HEROESFAMILY"
}

game:GetService("ReplicatedStorage").Knit.Services.CodesService.RF.Redeem:InvokeServer(unpack(args))

    local args = {
    [1] = "KLARION"
}

game:GetService("ReplicatedStorage").Knit.Services.CodesService.RF.Redeem:InvokeServer(unpack(args))

    local args = {
    [1] = "RAINBOW"
}

game:GetService("ReplicatedStorage").Knit.Services.CodesService.RF.Redeem:InvokeServer(unpack(args))
end)

PlayersSection:NewSlider("WalkSpeed", "Fast Or Slow", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

