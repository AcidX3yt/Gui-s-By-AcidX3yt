local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Heroes Online World Gui   AcidX3yt#7051", "Midnight")

local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")

local Tab = Window:NewTab("Keybinds")
local KeybindsSection = Tab:NewSection("Keybinds")

local Tab = Window:NewTab("Player")
local PlayersSection = Tab:NewSection("Player")

local Tab = Window:NewTab("Admin")
local AdminsSection = Tab:NewSection("Admins")

local Tab = Window:NewTab("Credits")
local CreditsSection = Tab:NewSection("Credits")

PlayersSection:NewSlider("WalkSpeed", "Fast Or Slow", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

KeybindsSection:NewKeybind("Etavitewal", "Zatanna Zattara", Enum.KeyCode.P, function()
	print("You just clicked the bind")
	local args = {
    [1] = "ExtraKey1",
    [2] = true,
    [3] = "CurrentAbility"
}

game:GetService("ReplicatedStorage").Knit.Services.AbilityService.RF.AbilityRequest:InvokeServer(unpack(args))

end)

KeybindsSection:NewKeybind("Tel og", "Zatanna Zattara", Enum.KeyCode.E, function()
	print("You just clicked the bind")
	local args = {
    [1] = "ExtraKey2",
    [2] = true,
    [3] = "CurrentAbility"
}

game:GetService("ReplicatedStorage").Knit.Services.AbilityService.RF.AbilityRequest:InvokeServer(unpack(args))

end)

KeybindsSection:NewKeybind("Pots gnihtaerb", "Zatanna Zattara", Enum.KeyCode.T, function()
	print("You just clicked the bind")
	local args = {
    [1] = "ExtraKey3",
    [2] = true,
    [3] = "CurrentAbility"
}

game:GetService("ReplicatedStorage").Knit.Services.AbilityService.RF.AbilityRequest:InvokeServer(unpack(args))

end)

KeybindsSection:NewKeybind("Laeh", "Zatanna Zattara", Enum.KeyCode.V, function()
	print("You just clicked the bind")
	local args = {
    [1] = "ExtraKey4",
    [2] = true,
    [3] = "CurrentAbility"
}

game:GetService("ReplicatedStorage").Knit.Services.AbilityService.RF.AbilityRequest:InvokeServer(unpack(args))

end)

AdminsSection:NewButton("Infinite yeild", "use ;", function()
    print("Clicked")
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

CreditsSection:NewLabel("Creator:AcidX3yt")
CreditsSection:NewLabel("Helper:Ajay")
CreditsSection:NewLabel("Helper:Chase")
CreditsSection:NewLabel("Helper:Zuv")

local Tab = Window:NewTab("Setting")
local SettingsSection = Tab:NewSection("Settings")

SettingsSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
