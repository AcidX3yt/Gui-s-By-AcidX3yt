PlayerSection:NewSlider("WalkSpeed", "Makes you move Faster or Slower", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MainSection:NewButton("Energy", "GIves Energy", function()
    print("Clicked")
local args = {
    [1] = "Energy",
    [2] = "1000",
    [3] = "-1"
}

game:GetService("ReplicatedStorage").Events.Buy:FireServer(unpack(args))
end)

